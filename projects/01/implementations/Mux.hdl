/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
        Not(in=sel, out=notSel);
        And(a=a, b=notSel, out=aOutput); 
        And(a=sel,b=b,out=bOutput);
        Or(a=aOutput, b=bOutput, out=out);
}